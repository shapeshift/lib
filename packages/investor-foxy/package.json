{
  "name": "@shapeshiftoss/investor-foxy",
  "version": "1.3.1",
  "description": "An API layer for interaction with the ShapeShift FOXy contract",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "dist",
    "src": "src"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14.18.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "yarn clean && tsc --project tsconfig.build.json",
    "clean": "rm -rf dist",
    "dev": "tsc --watch",
    "prepare": "yarn clean && yarn build",
    "test": "jest --coverage",
    "cli": "yarn build && node ./dist/foxycli.js"
  },
  "dependencies": {
    "@ethersproject/providers": "^5.5.3",
    "axios": "^0.26.1",
    "bignumber.js": "^9.0.2",
    "lodash": "^4.17.21",
    "web3": "^1.7.1",
    "web3-core": "^1.7.1",
    "web3-utils": "^1.7.1"
  },
  "peerDependencies": {
    "@shapeshiftoss/caip": "^2.0.1",
    "@shapeshiftoss/chain-adapters": "^2.0.0",
    "@shapeshiftoss/hdwallet-core": "^1.19.0",
    "@shapeshiftoss/types": "^2.1.0"
  },
  "devDependencies": {
    "@shapeshiftoss/caip": "^2.0.1",
    "@shapeshiftoss/chain-adapters": "^2.0.0",
    "@shapeshiftoss/hdwallet-core": "^1.19.0",
    "@shapeshiftoss/types": "^2.1.0"
