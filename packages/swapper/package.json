    "bignumber.js": "^9.1.1",
    "lodash": "^4.17.21",
    "retry-axios": "^2.6.0",
    "web3": "1.7.4"
  },
  "peerDependencies": {
    "@shapeshiftoss/asset-service": "^8.0.1",
